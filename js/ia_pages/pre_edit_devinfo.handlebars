<div class="dev-info-details" id="code">
    <table class="iadata">
        <tr>
            <td><strong>Id:</strong></td>
            <td>{{id}}</td>
        </tr>
        <tr>
            <td><strong>Templates:</strong></td>
            <td>
                list of templates here, if any, or
                default for {{repo}}
            </td>
        </tr>
        {{#if code}}
            <tr>
                <td><strong>Code:</strong></td>
                <td>
                    {{#each code}}
                        <li class="pre_edit" name="code">{{this}}</li>
                    {{/each}}
                </td>
            </tr>
        {{/if}}
        <tr>
            <td><strong>Credits:</strong></td>
            <td>
                {{#each attribution}}
                    <li><a href="{{this.0.loc}}">{{this.0.type}}</a></li>
                {{/each}}
            </td>
        </tr>
    </table>
</div>
