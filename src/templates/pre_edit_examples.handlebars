<div class="row-diff" id="row-diff-examples">
    <div class="column-left-labels left">
        <div class="column-left-labels__child">
            <p class="column-left-labels__child__p">
                Examples
            </p>
        </div>
    </div>
    <div class="column-center-live left">
        <div class="column-center-live__child">
            <ul class="column-center-live__child__ul">
                {{#if live.example_query}}
                    <li class="editpage">
                        <span title="try this example on DuckDuckGo" name="example_query" class="pre_edit" id="primary">
                            {{live.example_query}}
                        </span>
                    </li>
                {{/if}}
                {{#if live.other_queries}}
                    {{#each live.other_queries}}
                        <li class="editpage">
                            <span title="try this example on DuckDuckGo" name="other_queries" class="pre_edit other-examples">
                                {{this}}
                            </span>
                        </li>
                    {{/each}}
                {{/if}}
            </ul>
        </div>
    </div>
    <div class="column-right-edits left" id="column-edits-examples">
        <div class="column-right-edits__child" id="examples">
            <ul class="column-center-live__child__ul">
                {{#if edited.example_query}}
                    <li class="editpage">
                        <span title="try this example on DuckDuckGo" name="example_query" class="pre_edit" id="primary">
                            {{edited.example_query}}
                        </span>
                    </li>
                {{/if}}
                {{#if edited.other_queries}}
                    {{#each edited.other_queries}}
                        <li class="editpage">
                            <span title="try this example on DuckDuckGo" name="other_queries" class="pre_edit other-examples">
                                {{this}}
                            </span>
                        </li>
                    {{/each}}
                {{/if}}
            </ul>
        </div>
    </div>

    <div class="button js-pre-editable" name="examples">
        <i class="js-pre-editable__icon icon-edit"></i>
        <span class="js-pre-editable__text">Edit</span>
    </div>

    <div class="button hide js-editable" name="examples">
        <i class="js-editable__icon icon-check"></i>
        <span class="js-editable__text">Done</span>
    </div>
</div>
