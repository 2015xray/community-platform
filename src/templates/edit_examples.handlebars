<div class="column-left-labels left">
    <div class="column-left-labels__child">
        <p class="column-left-labels__child__p">
            Examples
        </p>
    </div>
</div>
<div class="column-center-live">
    <div class="column-center-live__child">
        <ul class="column-center-live__child__ul">
            {{#if live.example_query}}
                <li class="editpage">
                    <span title="try this example on DuckDuckGo" name="example_query" class="pre_edit" id="primary">{{example_query}}</span>
                </li>
            {{/if}}
            {{#if live.other_queries}}
                {{#each other_queries}}
                    <li class="editpage">
                        <span title="try this example on DuckDuckGo" name="other_queries" class="pre_edit other-examples">{{this}}</span>
                    </li>
                {{/each}}
            {{/if}}
        </ul>
    </div>
</div>
<div class="column-right-edits righ">
    <div class="column-right-edits__child js-section-editable" id="examples" name="examples">
        <ul>
            {{#if edited.example_query}}
                <li class="editpage">
                    <div class="button delete listbutton" name="example_query" title="Remove this example"><span>-</span></div>
                    <span title="try this example on DuckDuckGo" name="example_query" class="editable" id="primary">
                        <input type="text" value="{{edited.example_query}}" />
                    </span>
                </li>
            {{else}}
                <li class="editable" name="example_query" id="primary">
                    <input type="text" value="" />
                </li>
            {{/if}}
            {{#if edited.other_queries}}
                {{#each edited.other_queries}}
                    <li class="editpage">
                        <div class="button delete listbutton" name="other_queries" title="Remove this example"><span>-</span></div>
                        <span title="try this example on DuckDuckGo" name="other_queries" class="editable other-examples">
                            <input type="text" value="{{this}}" />
                        </span>
                    </li>
                {{/each}}
            {{/if}}
            <li class="editpage"><div name="other_queries" class="editable other-examples hide" id="input_example">
                <input type="text" value="" />
            </div></li>
            <li class="editpage"><div class="button listbutton" id="add_example" title="Add example"><span>+</span></div></li>
        </ul>
    </div>
    <div class="button end left">OK</div>
    <div class="button cancel left" name="examples">Cancel</div>
</div>

