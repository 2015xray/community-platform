{{#ne_and dev_milestone 'live' 'deprecated'}}
    {{#ne_and dev_milestone 'planning' 'development'}}
        <div class="left third frm">
            <div class="ia-single--header">
                Browser Testing
            </div>
            <label class="frm__label">
                <input data-panel="testing" id="browsers_ie-check" class="browsers_ie frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.browsers_ie}}checked="checked"{{/if}}>
                <span class="frm__label__txt">IE</span>
            </label>
            <label class="frm__label">
                <input data-panel="testing" id="browsers_chrome-check" class="browsers_chrome frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.browsers_chrome}}checked="checked"{{/if}}>
                <span class="frm__label__txt">Chrome</span>
            </label>
            <label class="frm__label">
                <input data-panel="testing" id="browsers_firefox-check" class="browsers_firefox frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.browsers_firefox}}checked="checked"{{/if}}>
                <span class="frm__label__txt">Firefox</span>
            </label>
            <label class="frm__label">
                <input data-panel="testing" id="browsers_safari-check" class="browsers_safari frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.browsers_safari}}checked="checked"{{/if}}>
                <span class="frm__label__txt">Safari</span>
            </label>
            <label class="frm__label">
                <input data-panel="testing" id="browsers_safari-check" class="browsers_opera frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.browsers_opera}}checked="checked"{{/if}}>
                <span class="frm__label__txt">Opera</span>
            </label>
        </div>

        <div class="left third frm">
            <div class="ia-single--header">
                Mobile Testing
            </div>
            <label class="frm__label">
                <input data-panel="testing" id="mobile_ios-check" class="mobile_ios frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.mobile_ios}}checked="checked"{{/if}}>
                <span class="frm__label__txt">iOS</span>
            </label>
            <label class="frm__label">
                <input data-panel="testing" id="mobile_android-check" class="mobile_android frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.mobile_android}}checked="checked"{{/if}}>
                <span class="frm__label__txt">Android</span>
            </label>
        </div>

        <div class="left third frm">
            <div class="ia-single--header">
                Miscellaneous
            </div>
            <label class="frm__label">
                <input data-panel="testing" class="unsafe frm__label__chk {{#if permissions.can_edit}}js-autocommit{{/if}}" type="checkbox" {{#if live.unsafe}}checked="checked"{{/if}}>
                <span class="frm__label__txt">Requires Safe Search</span>
            </label>
        </div>
    {{/ne_and}}
{{/ne_and}}
