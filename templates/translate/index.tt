<div class="buttonwrap">	<a class="button" href="<@ u('Help','view','translate') @>"><@ l('Instructions for our translation system') @></a></div><@ IF !c.user @>	<div class="mybox">		<@ l('You must be logged in for translating') @>		<a href="<@ u('My','login') @>"><@ l('Login') @></a>		<a href="<@ u('My','register') @>"><@ l('Register') @></a>	</div><@ ELSE @>	<@ FOR token_domain IN token_domains.all @>		<div class="mybox">			<a href="<@ u('Translate','domain',token_domain.key) @>"><@ token_domain.name @></a>			<@ IF token_domain.description @>				<div>					<@ token_domain.description @>				</div>			<@ END @>		</div>	<@ END @><@ END @>