<section class="menu-list tab-third">
	<h5>Topics</h5>
	<ul>
		<: for $blog_topics -> $topic { :>
			<li>
				<a href="<: $u('Blog','topic',urify($topic)) :>"><: $topic :>
				<a class="icon rss" href="<: $u('Blog','topic_rss',$topic) :>" title="DuckDuckGo Blog RSS feed about topic <: $topic :>"><i class="icon-rss-sign"></i></a>
			</li>
		<: } :>
	</ul>
</section>