<: if $page == $no { :>
	<span class="button button-nav-current disabled"><: $no_string ? $no_string : $no :></span>
<: } else if !$no { :>
	<span class="button button-nav-current disabled"><: $no_string :></span>
<: } else { :>
	<a class="button" href="<: replace($url, 'PAGE_NO', $no) :>"><: $no_string ? $no_string : $no :></a>
<: } :>
